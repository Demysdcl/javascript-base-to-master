{
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": true,
        "source.organizeImports": true
    },
    "code-runner.executorMap": {        
        "typescript": "npx ts-node --files --transpile-only",
    },
    "code-runner.clearPreviousOutput": true
}